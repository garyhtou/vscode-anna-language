{
	"name": "anna",
	"displayName": "ANNA",
	"description": "Language support and syntax highlighting for ANNA Assembly",
	"version": "0.0.1",
	"repository": "https://github.com/garyhtou/vscode-anna-language",
	"author": "Gary Tou <gary@garytou.com> (https://garytou.com)",
	"engines": {
		"vscode": "^1.62.0"
	},
	"categories": [
		"Programming Languages"
	],
	"contributes": {
		"languages": [
			{
				"id": "anna",
				"aliases": [
					"ANNA",
					"anna"
				],
				"extensions": [
					".ac"
				],
				"configuration": "./language-configuration.json"
			}
		],
		"grammars": [
			{
				"language": "anna",
				"scopeName": "source.anna",
				"path": "./syntaxes/anna.tmLanguage.json"
			}
		]
	}
}
